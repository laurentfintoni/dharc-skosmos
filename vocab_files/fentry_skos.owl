    <rdf:Description rdf:about="http://www.essepuntato.it/2014/03/fentry/describes">
        <skos:definition xml:lang="en">It allows one to link a Scheda F to the photograph and the portrayed object the Scheda F describes.</skos:definition>
        <rdfs:label xml:lang="en">describes</rdfs:label>
        <skos:prefLabel xml:lang="en">describes</skos:prefLabel>
    </rdf:Description>
